@inproceedings{citeulike:220163,
abstract = {In this paper we present a novel method for creating realistic, controllable motion. Given a corpus of motion capture data, we automatically construct a directed graph called a motion graph that encapsulates connections among the database. The motion graph consists both of pieces of original motion and automatically generated transitions. Motion can be generated simply by building walks on the graph. We present a general framework for extracting particular graph walks that meet a user's specifications. We then show how this framework can be applied to the specific problem of generating different styles of locomotion along arbitrary paths.},
address = {New York, NY, USA},
annote = {They construct a direct graph called motion graph that encapsulates connections among the database, containing original motions and automatically generated ones, which are the edges, and the nodes are choice points. It's also possible to add constrains to specify certain points along the path where the character must act in a specific way. They generate motion by piecing together example motions from the database. Move trees are a similar idea close to motion graphs, but created manually. To create transitions: identify portions of the initial clips that are similar → blending. To detect candidate transitions they measure the distance between two frames in terms of a point cloud driven by the skeleton → distance between frames. They create a error matrix between any two frames, and the local minima are the transition candidates. They set up a threshold. They apply lineal transformations and interpolate the joints to create transitions and blend. Problems: Sink and deadlocks are fixed using strong connected graphs. Artifacts due blending are fixed with constraint information. The user specifies a scalar function f(g,w) that evaluates the additional error accrued by appending an edge e to the existing path w, which my be the empty path ө → appends edges to create a graph The user also specifies a halt condition. Important observations: g should give sort of guidances and g shouldn't be too restrictive. Good points: Automatically constructed graphs that encapsulates connection among motions Bad points: The main bottleneck is locating the candidate transitions, O(Frames\^{}2) operations, though it's easy to parallelize. The transition threshold has to be specified by hand.},
author = {Kovar, Lucas and Gleicher, Michael and Pighin, Fr\'{e}d\'{e}ric},
booktitle = {ACM Trans. on Graphics},
file = {:C$\backslash$:/Users/stonneau/AppData/Local/Mendeley Ltd/Mendeley Desktop/Downloaded/mograph.pdf:pdf},
isbn = {1-58113-521-1},
issn = {0730-0301},
keywords = {distance,error\_matrix,graphs,interpolate,motion,threshold},
publisher = {ACM},
title = {{Motion graphs}},
volume = {21},
year = {2002}
}
@article{Baerlocher2004,
